  "components": {
    "schemas": {
      "CheckPasswordRequestModel": {
        "required": [
          "password",
          "personId"
        ],
        "type": "object",
        "properties": {
          "personId": {
            "type": "integer",
            "format": "int32"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CreateCarRequestModel": {
        "required": [
          "bodyName",
          "brandName",
          "driveName",
          "engineCapacityL",
          "enginePowerHP",
          "enginePowerKW",
          "engineTypeName",
          "fuelTypeName",
          "gearboxName",
          "modelName",
          "personId",
          "releaseYear",
          "tankCapacityL",
          "vehicleWeightKG",
          "vinNumber"
        ],
        "type": "object",
        "properties": {
          "personId": {
            "type": "integer",
            "format": "int32"
          },
          "vinNumber": {
            "maxLength": 17,
            "minLength": 17,
            "type": "string"
          },
          "stateNumber": {
            "pattern": "^[авекмнорстухАВЕКМНОРСТУХ][0-9]{3}[авекмнорстухАВЕКМНОРСТУХ]{2}[0-9]{2,3}$",
            "type": "string",
            "nullable": true
          },
          "brandName": {
            "maxLength": 30,
            "minLength": 0,
            "type": "string"
          },
          "modelName": {
            "maxLength": 30,
            "minLength": 0,
            "type": "string"
          },
          "bodyName": {
            "maxLength": 30,
            "minLength": 0,
            "type": "string"
          },
          "releaseYear": {
            "pattern": "^20[0-9]{2}$",
            "type": "integer",
            "format": "int32"
          },
          "gearboxName": {
            "maxLength": 30,
            "minLength": 0,
            "type": "string"
          },
          "driveName": {
            "maxLength": 30,
            "minLength": 0,
            "type": "string"
          },
          "vehicleWeightKG": {
            "type": "integer",
            "format": "int32"
          },
          "enginePowerHP": {
            "type": "integer",
            "format": "int32"
          },
          "enginePowerKW": {
            "type": "number",
            "format": "float"
          },
          "engineCapacityL": {
            "type": "number",
            "format": "float"
          },
          "engineTypeName": {
            "maxLength": 30,
            "minLength": 0,
            "type": "string"
          },
          "tankCapacityL": {
            "type": "integer",
            "format": "int32"
          },
          "fuelTypeName": {
            "maxLength": 30,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CreatePersonRequestModel": {
        "required": [
          "birth",
          "email",
          "firstName",
          "lastName",
          "password",
          "phone",
          "rightLevel"
        ],
        "type": "object",
        "properties": {
          "email": {
            "maxLength": 320,
            "minLength": 0,
            "type": "string",
            "format": "email"
          },
          "phone": {
            "minLength": 1,
            "pattern": "^\\+7[0-9]{10}$",
            "type": "string"
          },
          "lastName": {
            "maxLength": 50,
            "minLength": 2,
            "type": "string"
          },
          "firstName": {
            "maxLength": 50,
            "minLength": 2,
            "type": "string"
          },
          "patronymic": {
            "maxLength": 50,
            "minLength": 2,
            "type": "string",
            "nullable": true
          },
          "birth": {
            "type": "string",
            "format": "date"
          },
          "password": {
            "maxLength": 32,
            "minLength": 8,
            "type": "string"
          },
          "driveLisense": {
            "maxLength": 10,
            "minLength": 10,
            "type": "string",
            "nullable": true
          },
          "rightLevel": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "EndTripRequestModel": {
        "required": [
          "endDatetime"
        ],
        "type": "object",
        "properties": {
          "endDatetime": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "StartTripRequestModel": {
        "required": [
          "carId",
          "macAddress",
          "startDatetime"
        ],
        "type": "object",
        "properties": {
          "startDatetime": {
            "type": "string",
            "format": "date-time"
          },
          "macAddress": {
            "minLength": 1,
            "pattern": "[0-9a-fA-F]:[0-9a-fA-F]:[0-9a-fA-F]:[0-9a-fA-F]:[0-9a-fA-F]:[0-9a-fA-F]",
            "type": "string"
          },
          "carId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    }
  }
